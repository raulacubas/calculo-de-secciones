<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/mul3"
        android:layout_width="92dp"
        android:layout_height="69dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:gravity="center"
        android:hint="0"
        android:inputType="numberDecimal"
        android:text="1"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/num2"
        android:layout_width="206dp"
        android:layout_height="68dp"
        android:ems="10"
        android:gravity="center"
        android:hint="0"
        android:inputType="numberDecimal"
        android:text="0"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mul2"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.328"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/num1"
        android:layout_width="206dp"
        android:layout_height="68dp"
        android:ems="10"
        android:gravity="center"
        android:hint="0"
        android:inputType="numberDecimal"
        android:text="0"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mul1"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.168"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/num3"
        android:layout_width="206dp"
        android:layout_height="68dp"
        android:ems="10"
        android:gravity="center"
        android:hint="0"
        android:inputType="numberDecimal"
        android:text="0"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mul3"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/mul1"
        android:layout_width="92dp"
        android:layout_height="69dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:gravity="center"
        android:hint="0"
        android:inputType="numberDecimal"
        android:text="1"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/mul2"
        android:layout_width="92dp"
        android:layout_height="69dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:gravity="center"
        android:hint="0"
        android:inputType="numberDecimal"
        android:text="1"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.327"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnarea"
        android:layout_width="134dp"
        android:layout_height="59dp"
        android:text="Calcular"
        app:layout_constraintBottom_toTopOf="@+id/textres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num1"
        app:layout_constraintVertical_bias="0.927" />

    <TextView
        android:id="@+id/textres"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="0"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num1"
        app:layout_constraintVertical_bias="0.797"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/textres2"
        android:layout_width="207dp"
        android:layout_height="63dp"
        android:layout_marginStart="40dp"
        android:gravity="center"
        android:text="Diametro"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/num1"
        app:layout_constraintEnd_toStartOf="@+id/textres3"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.684" />

    <TextView
        android:id="@+id/textres3"
        android:layout_width="127dp"
        android:layout_height="57dp"
        android:layout_marginEnd="16dp"
        android:gravity="center"
        android:text="Cantidad"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/num1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.59" />

    <TextView
        android:id="@+id/secalam1"
        android:layout_width="200dp"
        android:layout_height="19dp"
        android:text="Seccion alambre 1"
        app:layout_constraintBottom_toTopOf="@+id/num2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num1"
        app:layout_constraintVertical_bias="0.45" />

    <TextView
        android:id="@+id/secalam2"
        android:layout_width="197dp"
        android:layout_height="17dp"
        android:text="Seccion alambre 2"
        app:layout_constraintBottom_toTopOf="@+id/num3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num2"
        app:layout_constraintVertical_bias="0.535" />

    <TextView
        android:id="@+id/secalam3"
        android:layout_width="198dp"
        android:layout_height="18dp"
        android:text="Seccion alambre 3"
        app:layout_constraintBottom_toTopOf="@+id/btnarea"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.206"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num3"
        app:layout_constraintVertical_bias="0.321" />


</androidx.constraintlayout.widget.ConstraintLayout>